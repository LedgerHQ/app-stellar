{
    "env": {
        "BOLOS_SDK": "/opt/nanosplus-secure-sdk"
    },
    "configurations": [
        {
            "name": "Nano SP",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/libstellar/include",
                "/usr/include",
                "${env:BOLOS_SDK}/include",
                "${env:BOLOS_SDK}/lib_cxng/include",
                "${env:BOLOS_SDK}/lib_ux/include",
                "${env:BOLOS_SDK}/lib_standard_app",
            ],
            "defines": [
                "TARGET_NANOSP",
                "OS_IO_SEPROXYHAL",
                "HAVE_BAGL",
                "HAVE_NBGL",
                "HAVE_SPRINTF",
                "HAVE_IO_USB",
                "HAVE_L4_USBLIB",
                "IO_USB_MAX_ENDPOINTS=6",
                "IO_HID_EP_LENGTH=64",
                "HAVE_USB_APDU",
                "USB_SEGMENT_SIZE=64",
                "UNUSED(x)=(void)x",
                "APPNAME=\"Stellar\"",
                "MAJOR_VERSION=1",
                "MINOR_VERSION=0",
                "PATCH_VERSION=0",
                "IO_SEPROXYHAL_BUFFER_SIZE_B=128",
                "HAVE_UX_FLOW",
                "DEBUG=1",
                "HAVE_PRINTF",
                "PRINTF=screen_printf"
            ],
            "compilerPath": "/opt/gcc-arm-none-eabi-10.3-2021.10/bin/arm-none-eabi-gcc",
            "cStandard": "gnu17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}